<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: B04759;" stylesheets="@style.css" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.controllers.LogoutController">
    <children>
        <VBox layoutX="111">
            <children>
                <Label prefWidth="178.0" text="Are you sure you want to logout?" textFill="WHITE" />        
            </children>
        </VBox>
        <ButtonBar layoutX="150.0" layoutY="20.0" fx:id="buttonBar" prefHeight="40.0" prefWidth="30.0">
            <buttons>
                <Button layoutX="-300.0" fx:id="logoutButton" mnemonicParsing="false" onMouseClicked="#logout" text="Logout" />
            </buttons>
        </ButtonBar>
    </children>
</AnchorPane>
